{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "hipsterbrown:dofbot",
  "visibility": "public",
  "url": "https://github.com/hipsterbrown/viam-yahboom-dofbot",
  "description": "Control the arm servos and gripper of the Yahboom Dofbot",
  "models": [
    {
      "api": "rdk:component:arm",
      "model": "hipsterbrown:dofbot:arm",
      "short_description": "Control the joint position and end effector pose of the Yahboom Dofbot",
      "markdown_link": "README.md#model-hipsterbrowndofbotarm"
    },
    {
      "api": "rdk:component:gripper",
      "model": "hipsterbrown:dofbot:gripper",
      "short_description": "Control the gripper of the Yahboom Dofbot",
      "markdown_link": "README.md#model-hipsterbrowndofbotgripper"
    }
  ],
  "applications": null,
  "markdown_link": "README.md",
  "entrypoint": "bin/dofbot",
  "first_run": "",
  "build": {
    "build": "make module.tar.gz",
    "setup": "make setup",
    "path": "module.tar.gz",
    "arch": [
      "linux/arm64"
    ]
  }
}
